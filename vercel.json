{
    "buildCommand": "next build",
    "framework": "nextjs",
    "crons": [
        {
            "path": "/api/cron/evaluate-alerts",
            "schedule": "*/5 * * * *"
        },
        {
            "path": "/api/cron/hourly-check",
            "schedule": "0 * * * *"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}